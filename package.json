        "protobufjs": "^6.11.2",
        "sass": "^1.49.0",
        "vue": "^3.2.25",
        "vue-echarts": "^6.2.3",
        "vue-i18n": "^9.1.10",
        "vue-router": "4",
        "vue-virtual-scroll-grid": "^1.4.2",
        "vuex": "4"
    },
    "devDependencies": {
        "@types/node": "^17.0.23",
        "@vitejs/plugin-vue": "^2.0.0",
        "typescript": "^4.4.4",
        "unplugin-auto-import": "^0.5.11",
